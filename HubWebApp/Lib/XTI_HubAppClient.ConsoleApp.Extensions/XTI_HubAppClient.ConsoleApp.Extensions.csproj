<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Description>Console App Extensions using $(XtiAppName) $(XtiVersion)</Description>
		<PackageTags>Console App;$(XtiAppName);$(XtiVersion);</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="XTI_App.Extensions" Version="1.8.48" />
		<PackageReference Include="XTI_App.Hosting" Version="1.8.48" />
		<PackageReference Include="XTI_ConsoleApp" Version="1.8.48" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\XTI_HubAppClient.Extensions\XTI_HubAppClient.Extensions.csproj" />
	</ItemGroup>

</Project>
