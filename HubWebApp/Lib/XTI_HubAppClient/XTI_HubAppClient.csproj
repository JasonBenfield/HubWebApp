		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<Description>App Client for $(XtiAppName) $(XtiVersion)</Description>
		<PackageTags>App Client;$(XtiAppName);$(XtiVersion);</PackageTags>
	</PropertyGroup>
	
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="8.0.0" />
		<PackageReference Include="XTI_App.Api" Version="1.8.42" />
