<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<Description>Setup App Extensions using $(XtiAppName) $(XtiVersion)</Description>
		<PackageTags>Setup App;$(XtiAppName);$(XtiAppType);$(XtiVersion);</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="9.0.0" />
		<PackageReference Include="XTI_Secrets.Extensions" Version="1.8.13" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\XTI_HubAppClient.Extensions\XTI_HubAppClient.Extensions.csproj" />
	</ItemGroup>

</Project>
