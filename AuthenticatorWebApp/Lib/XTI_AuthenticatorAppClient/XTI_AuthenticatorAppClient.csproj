<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<Description>App Client for $(XtiAppName) $(XtiVersion)</Description>
		<PackageTags>App Client;$(XtiAppName);$(XtiVersion);</PackageTags>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="XTI_App.Api" Version="1.8.39" />
		<PackageReference Include="XTI_WebAppClient" Version="1.8.39" />
		<PackageReference Include="XTI_TempLog.Abstractions" Version="1.2.31" />
	</ItemGroup>

</Project>